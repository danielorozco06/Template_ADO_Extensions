{
  "manifestVersion": 1,
  "id": "ai-devops-tools",
  "version": "1.0.0",
  "name": "ai-devops-tools",
  "description": "AI tools for azure pipelines",
  "publisher": "DevOps Team",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "BuildNumber"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": [
    "vso.build",
    "vso.code_full",
    "vso.work",
    "vso.release"
  ],
  "icons": {
    "default": "assets/logo.png"
  },
  "content": {
    "details": {
      "path": "assets/overview.md"
    }
  },
  "contributions": [
    {
      "id": "pullrequest-verifier-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "dist/"
      }
    }
  ],
  "files": [
    {
      "path": "dist/"
    }
  ]
}